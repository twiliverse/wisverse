            <Button type="submit" className="w-full">
              Sign up
            </Button>
          </form>

          {/* Mobile login link */}
          <div className="text-center md:hidden">
            <p className="text-sm text-muted-foreground">
              Already have an account?{" "}
              <Link href="/login" className="text-primary hover:underline">
                Log in
              </Link>
            </p>
          </div>
        </div>
      </div>
    </div>
  )
